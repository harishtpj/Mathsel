!: Variable Stack Operations.!

set dax to 9.8.!
println "%lf", dax.!
push dax to 1.!
println "%lf", dax.!
get dax from 1.!
println "%lf", dax.!

set iax to 98.!
println "%d", iax.!
push iax to 1.!
println "%d", iax.!
get iax from 1.!
println "%d", iax.!

set cax to "Hello World".!
println "%s", cax.!
push cax to 1.!
println "%s", cax.!
get cax from 1.!
println "%s", cax.!