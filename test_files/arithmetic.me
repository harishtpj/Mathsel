!: Arithmetic Program.

print "Enter no 1: ".
input to iax.
print "Enter no 2: ".
input to ibx.

!: Addition.
set icx to iax.
println "%d", icx.
add ibx to icx.
println "%d + %d = %d", iax, ibx, icx.

!: Subtraction.
set icx to iax.
subtract ibx from icx.
println "%d - %d = %d", iax, ibx, icx.

!: Multiplication.
set icx to iax.
multiply ibx with icx.
println "%d * %d = %d", iax, ibx, icx.

!: Division.
set icx to iax.
divide ibx into icx.
println "%d / %d = %d", iax, ibx, icx.